{
    "name": "yiistack/inertia",
    "type": "library",
    "description": "Yii Inertia Library",
    "keywords": [
        "yii3",
        "inertia"
    ],
    "homepage": "https://github.com/yiistack",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/yiistack/inertia/issues?state=open",
        "source": "https://github.com/yiistack/inertia"
    },
    "require": {
        "php": "^7.4|^8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.4",
        "roave/infection-static-analysis-plugin": "^1.5",
        "spatie/phpunit-watcher": "^1.23",
        "vimeo/psalm": "^4.2"
    },
    "autoload": {
        "psr-4": {
            "Yiistack\\Inertia\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Yiistack\\Inertia\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "scripts": {
        "test": "phpunit --testdox --no-interaction",
        "test-watch": "phpunit-watcher watch"
    },
    "config": {
        "sort-packages": true
    }
}
